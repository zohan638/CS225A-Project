<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 0.0">

	<robot name="ocean1">
		<model dir="${CS225A_URDF_FOLDER}/ocean1" path="ocean1.urdf" name="ocean1" />
		<!-- <origin xyz="0 0 -0.94" rpy="0 0 0" /> -->
	</robot>

	<static_object name="Ground_With_Texture">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	    <visual>
	        <origin xyz="0.0 0.0 -0.0001" rpy="0 0 0" />
	        <geometry>
	            <box size="2 2 0.0002" />
	        </geometry>
	        <material name="material">
		        <color rgba="0.5 0.5 0.5 1.0" />
	        </material>
	    </visual>
		<collision>
	        <origin xyz="0.0 0.0 -0.0001" rpy="0 0 0" />
	        <geometry>
	            <box size="2 2 0.0002" />
	        </geometry>
	    </collision>
	</static_object>

	<dynamic_object name="cup">
		<origin xyz="0.0 0.5 1.0" rpy="0 0 0" />
		<inertial>
        	<origin xyz="0 0 0" rpy="0 0 0" />
        	<mass value="10" />
        	<inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0" />
      	</inertial>

	    <visual>
	        <origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
	            <mesh filename="${CS225A_URDF_FOLDER}/test_objects/meshes/visual/cup.obj" />
	        </geometry>
	    </visual>

		<visual>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	        <material name="material">
		        <color rgba="1 1 1 1.0" />
	        </material>
	    </visual>

	    <collision>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	    </collision>
	</dynamic_object>

	<dynamic_object name="bottle">
		<origin xyz="0.8 0.0 -0.5" rpy="0 0 0" />
		<inertial>
        	<origin xyz="0 0 0" rpy="0 0 0" />
        	<mass value="10" />
        	<inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0" />
      	</inertial>

	    <!-- <visual>
	        <origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
	            <mesh filename="${CS225A_URDF_FOLDER}/test_objects/meshes/visual/bottle.obj" />
	        </geometry>
	    </visual> -->

		<visual>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	        <material name="material">
		        <color rgba="1 1 1 1.0" />
	        </material>
	    </visual>

	    <collision>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	    </collision>
	</dynamic_object>

    <dynamic_object name="welcome_sign">
        <origin xyz="-2.1 1 0" rpy="1.571 0 -1.571" />
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="5" />
            <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0" ixz="0" iyz="0" />
        </inertial>
        <visual>
            <origin xyz="0 0 0 " rpy="0 0 0" />
            <geometry>
                <mesh filename="${CS225A_URDF_FOLDER}/test_objects/meshes/visual/welcome_sign.obj" />
            </geometry>
            <material name="material">
                <color rgba="0.66 0.95 16 0.8" />
            </material>
        </visual>
    </dynamic_object>

	<light name="light1" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="-2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="4.0 0.0 0.0" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</camera>

</world>